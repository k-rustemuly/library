<!DOCTYPE html>
<html lang="en">
	<head>
		{{ assets({files: ['assets/plugins/custom/prismjs/prismjs.bundle.css'],  name: 'prismjs.bundle.css'}) }}
		{% include 'head.twig' %}
	</head>
	<body id="kt_body" class="header-fixed header-tablet-and-mobile-fixed toolbar-enabled toolbar-fixed" style="--kt-toolbar-height:55px;--kt-toolbar-height-tablet-and-mobile:55px">
		<div class="d-flex flex-column flex-root">
			<div class="page d-flex flex-row flex-column-fluid">
				<div class="wrapper d-flex flex-column flex-row-fluid" id="kt_wrapper">
					{% include 'panel/header.twig' %}
					<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
						{% include 'panel/toolbar.twig' %}
						<div class="post d-flex flex-column-fluid" id="kt_post">
							<div id="kt_content_container" class="container-xxl">
								<div class="card">
									<div class="card-header pt-5">
										<h3 class="card-title align-items-start flex-column">
											<span class="card-label fw-bolder text-gray-800">Mining Status</span>
											<span class="text-gray-400 mt-1 fw-bold fs-6">8k social visitors</span>
										</h3>
										<div class="card-toolbar">
											<div class="d-flex justify-content-end">
												<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#kt_modal_add">
													<span class="svg-icon svg-icon-2"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
													<rect opacity="0.5" x="11.364" y="20.364" width="16" height="2" rx="1" transform="rotate(-90 11.364 20.364)" fill="black"></rect>
													<rect x="4.36396" y="11.364" width="16" height="2" rx="1" fill="black"></rect>
												</svg></span>
													{{ add }}
												</button>
											</div>
										</div>
									</div>
									<div class="card-body">
										<div class="d-flex flex-stack">
											<div class="d-flex align-items-center me-5">
												<div class="symbol symbol-40px me-3">
													<span class="symbol-label bg-light-info">
														<span class="svg-icon svg-icon-2x svg-icon-info">
															<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
																<path d="M16.925 3.90078V8.00077L12.025 10.8008V5.10078L15.525 3.10078C16.125 2.80078 16.925 3.20078 16.925 3.90078ZM2.525 13.5008L6.025 15.5008L10.925 12.7008L6.025 9.90078L2.525 11.9008C1.825 12.3008 1.825 13.2008 2.525 13.5008ZM18.025 19.7008V15.6008L13.125 12.8008V18.5008L16.625 20.5008C17.225 20.8008 18.025 20.4008 18.025 19.7008Z" fill="black"></path>
																<path opacity="0.3" d="M8.52499 3.10078L12.025 5.10078V10.8008L7.125 8.00077V3.90078C7.125 3.20078 7.92499 2.80078 8.52499 3.10078ZM7.42499 20.5008L10.925 18.5008V12.8008L6.02499 15.6008V19.7008C6.02499 20.4008 6.82499 20.8008 7.42499 20.5008ZM21.525 11.9008L18.025 9.90078L13.125 12.7008L18.025 15.5008L21.525 13.5008C22.225 13.2008 22.225 12.3008 21.525 11.9008Z" fill="black"></path>
															</svg>
														</span>
													</span>
												</div>
												<div class="me-5">
													<a href="#" class="text-gray-800 fw-bolder text-hover-primary fs-6">GPUs mining</a>
													<span class="fw-bold fs-7 d-block text-start text-success ps-0">Running</span>
												</div>
											</div>
											<div class="d-flex align-items-center">
												<div class="d-flex flex-center">
													<div class="form-check form-switch form-check-custom form-check-solid">
														<input class="form-check-input h-20px w-30px" type="checkbox" value="" id="flexSwitchChecked" checked="checked">
													</div>
												</div>
											</div>
										</div>
										<div class="separator separator-dashed my-4"></div>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="modal fade" id="kt_modal_add" tabindex="-1" style="display: none;" aria-hidden="true">
						<div class="modal-dialog modal-dialog-centered mw-650px">
							<div class="modal-content">
								<div class="modal-header" id="kt_modal_add_header">
									<h2 class="fw-bolder">{{ add_new }}</h2>
									<div class="btn btn-icon btn-sm btn-active-icon-primary" data-bs-dismiss="modal">
										<span class="svg-icon svg-icon-1">
											<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
												<rect opacity="0.5" x="6" y="17.3137" width="16" height="2" rx="1" transform="rotate(-45 6 17.3137)" fill="black"></rect>
												<rect x="7.41422" y="6" width="16" height="2" rx="1" transform="rotate(45 7.41422 6)" fill="black"></rect>
											</svg>
										</span>
									</div>
								</div>
								<div class="modal-body scroll-y mx-5 mx-xl-15 my-7">
									
								</div>
							</div>
						</div>
					</div>
					{% include 'panel/footer.twig' %}
				</div>
			</div>
		</div>
		{% include 'panel/scrolltop.twig' %}
		{% include 'script.twig' %}
		{{ assets({files: ['assets/plugins/custom/prismjs/prismjs.bundle.js'],  name: 'prismjs.bundle.js'}) }}
		{{ assets({files: ['assets/plugins/custom/datatables/datatables.bundle.js'],  name: 'datatables.bundle.js'}) }}
	</body>
</html>